void main() {
    
    Map<int, Map<String, String>> Students = {
        1:{'name':'Mifta','Course':'Math'},
        2:{'name':'Roshni','Course':'Physics'},
        3:{'name':'Tonima','Course':'CHemistry'},
        4:{'name':'Tamim','Course':'Biology'},
        5:{'name':'Uthsho','Course':'Computer Science'}
    };
    print('Students With Courses:$Students');
    
    
    Students[6]={'name':'Shafin','Course':'History'};
    
    
    print('Updated Students List (added Student with course):$Students');
    
    
}
